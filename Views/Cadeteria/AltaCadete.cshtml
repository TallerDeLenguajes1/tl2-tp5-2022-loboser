@model tl2_tp4_2022_loboser.ViewModels.AltaCadeteViewModel

@{
    ViewData["Title"] = "AltaCadete";
}

<h1>@ViewData["Title"]</h1>

<div>
    <form asp-action="AltaCadete" method="post">
        <label asp-for="Nombre">Nombre
        <input asp-for="Nombre"></label><br>
        <label asp-for="Direccion">Direccion
        <input asp-for="Direccion"></label><br>
        <label asp-for="Telefono">Telefono
        <input asp-for="Telefono"></label><br>
        <button type="submit">Enviar</button>
    </form>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}