@model tl2_tp4_2022_loboser.ViewModels.UsuarioViewModel

@{
    ViewData["Title"] = "Pagina de Logeo";
}

<div class="text-center">
    <form asp-action="Logear" method="post">
        <label asp-for="Nombre">Nombre
        <input asp-for="Nombre"></label><br>
        <label asp-for="Pass">Contraseña
        <input asp-for="Pass" type="password"></label><br>
        <button type="submit">Logear</button>
    </form>
</div>
